{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "trace": true,
      "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-user-data"
    },
    {
      "name": "Attach to Chrome",
      "request": "attach",
      "type": "chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}